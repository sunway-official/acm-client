{
  "name": "acm-client",
  "version": "0.1.1",
  "private": true,
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sunway-team/acm-client.git"
  },
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "lint": "eslint App/**/*.js",
    "lint-staged": "lint-staged",
    "rndebugger-open": "rndebugger-open",
    "fix-react-navigation": "cp -r Lib/react-navigation node_modules",
    "copy-env": "cp env.example.development.js env.js",
    "postinstall": "run-p rndebugger-open fix-react-navigation copy-env",
    "precommit": "run-s lint-staged lint",
    "prettier-eslint:fix:App": "prettier-eslint \"App/**/*.js\" --single-quote --trailing-comma all --print-width 80 --tab-width 2 --write",
    "prettier-eslint:fix": "run-s prettier-eslint:fix:App"
  },
  "lint-staged": {
    "App/**/*.js": [
      "prettier --single-quote --trailing-comma all --print-width 80 --tab-width 2 --write",
      "git add"
    ]
  },
  "dependencies": {
    "add-graphql-subscriptions": "^1.0.1",
    "expo": "22.0.0",
    "graphql": "^0.10.5",
    "immutable": "^4.0.0-rc.9",
    "moment": "^2.18.1",
    "prettier-eslint-cli": "^4.4.0",
    "prop-types": "^15.5.10",
    "react": "16.0.0-beta.5",
    "react-apollo": "^1.4.15",
    "react-native": "github:expo/react-native#sdk-22.0.0",
    "react-native-animatable": "^1.2.4",
    "react-native-datepicker": "^1.6.0",
    "react-native-elements": "^0.17.0",
    "react-native-side-menu": "^1.1.3",
    "react-native-vector-icons": "^4.4.2",
    "react-navigation": "^1.0.0-beta.15",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-form": "^7.1.1",
    "redux-thunk": "^2.2.0",
    "subscriptions-transport-ws": "^0.9.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-plugin-root-import": "^5.1.0",
    "eslint": "^4.5.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-import-resolver-babel-root-import": "0.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-react-native": "^3.0.1",
    "husky": "^0.14.3",
    "jest-expo": "~20.0.0",
    "lint-staged": "^4.0.4",
    "npm-run-all": "^4.1.1",
    "overwrite": "^1.0.3",
    "prettier": "^1.7.4",
    "prettier-eslint": "^6.4.3",
    "react-native-debugger-open": "^0.3.12",
    "react-native-scripts": "1.2.1",
    "react-test-renderer": "16.0.0-alpha.12",
    "redux-devtools-extension": "^2.13.2"
  },
  "jest": {
    "preset": "jest-expo"
  }
}
